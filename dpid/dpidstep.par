# dcsloop parameters
# $Id: dpidstep.par,v 1.1 2001-04-22 19:22:27 vlad Exp $

########################################
# Type of controller
#contr_kind = nnc
contr_kind = lin

########################################
# Mode of the modeling
#input_kind = stream
input_kind = file

# (input_kind = stream) Length of series
stream_len = 5000

# Linear plant transfer function
linplant_tf = plant.tf

# (contr_kind = lin) Linear controller transfer function
lincontr_tf = dpid.tf

# (contr_kind = nnc) Neural net controller
nncontr = nnc.nn

# (input_kind = stream) Input transfer functions
refer_tf = refer.tf
noise_tf = noise.tf

# (input_kind = file) Input files
in_r = step.dat
in_n = zero.dat

# Output files
out_u = u.dat
out_e = e.dat
out_y = resp.dat
out_ny = ny.dat

# (input_kind = stream) Output files
out_r = r_out.dat
out_n = n_out.dat
